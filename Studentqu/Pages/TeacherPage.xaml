<Page x:Class="Studentqu.Pages.TeacherPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Studentqu.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="TeacherPage">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="150"/>
            <RowDefinition Height="150"/>
            <RowDefinition Height="150"/>
        </Grid.RowDefinitions>
        <Button x:Name="question" Grid.Row="0" Content="Вопросы" Click="question_Click"/>
        <Button x:Name="test_request" Grid.Row="1" Content="отчёты" Click="test_request_Click"/>
        <Button x:Name="students" Grid.Row="2" Content="студенты" Click="students_Click"/>
    </Grid>
</Page>
